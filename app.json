{
  "expo": {
    "name": "MediTrace",
    "slug": "MediTrace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.oluwanbowa.MediTrace"
    },
    "web": {
      "favicon": ""
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload a profile image."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notificationsound1.mp3",
            "./assets/sounds/notificationsound2.mp3",
            "./assets/sounds/notificationsound4.wav",
            "./assets/sounds/notificationsound5.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "92cf71bd-62f5-431e-8a60-b975c535ad14"
      }
    }
  }
}
