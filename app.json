{
  "expo": {
    "name": "MediTrace",
    "slug": "MediTrace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/meditracelogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/meditracelogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/meditracelogo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload a profile image."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/profilepicture.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification-sound1.mp3",
            "./assets/sounds/notification-sound2.mp3",
            "./assets/sounds/notification-sound4.wav",
            "./assets/sounds/notification-sound5.wav"
          ]
        }
      ]
    ]
  }
}
